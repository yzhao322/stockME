<div class="container">
    
        <div style="text-align: center;">
        <h1>Welcome to Master Manager page!</h1>
        <ul>
            {{#each usermember}}
            <p id="member">{{this.email}} - {{this.title}}
                <form class="upgrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default" id="upgrade" >Upgrade to Manager!
                    </button> </form>
            </p>



            {{/each}}

        </ul>
        </div>
  

    
        <div style="text-align: center;">
        <ul>
            {{#each usermanager}}
            <p id="manager">{{this.email}} - {{this.title}}
                <form class="degrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default" id="degrade">Degrade to Manager!
                    </button> </form></p>
            {{/each}}

        </ul>
        </div>
 
</div>
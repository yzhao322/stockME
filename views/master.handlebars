<div class="row" style="text-align: center;">
    <h1>Master page</h1>

    <div class="col-sm-6">
        <ul>
            {{#each usermember}}
            <p id="member">{{this.email}} - {{this.title}}
                <form class="upgrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default submit" id="upgrade">Upgrade to Manager!
                    </button> </form>
            </p>



            {{/each}}

        </ul>
        </div>
  

    
        <div style="text-align: center;">
        <ul>
            {{#each usermanager}}
            <p id="manager">{{this.email}} - {{this.title}}
                <form class="degrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default submit" id="degrade">Degrade to Member!
                    </button> </form>
            </p>
            {{/each}}

        </ul>
    </div>

     <div id="myModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content">
    <div class="modal-header">
      <span class="close">&times;</span>
      <h2>Successed!</h2>
    </div>

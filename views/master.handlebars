<h1>Welcome to manager page!</h1>

<div class="row">
    <div class="col-sm-6">
        <ul>
            {{#each usermember}}
            <p id="member">{{this.email}} - {{this.title}}
                <form class="upgrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default" id="upgrade" >Upgrade to Manager!
                    </button> </form>
            </p>



            {{/each}}

        </ul>
    </div>

    <div class="col-sm-6">
        <ul>
            {{#each usermanager}}
            <p id="manager">{{this.email}} - {{this.title}}
                <form class="degrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default" id="degrade">Upgrade to Manager!
                    </button> </form></p>
            {{/each}}

        </ul>
    </div>
</div>
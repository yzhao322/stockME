<nav class="navbar navbar-default" style="text-align: center;background-color:#666699">
    <div class="container">
        <div class="row">
            <div class="col-sm-3" style="float:left">
                
            </div>
            <div class="col-sm-6">
                <h1 class="stockMe-logo">StockME</h1>
                <hr>
                <h4 style="color: white;">Master page</h4>
            </div>
            <div class="col-sm-3" style="float:right"></div>

        </div>
    </div>
</nav>

<div class="row" style="text-align: center;border: 5px dashed #bababa;">
    

    <div class="col-sm-6">
        <h3 class="master-list">Member List</h3>
        <br>
        <ul>
            {{#each usermember}}
            <p id="member">{{this.email}} - {{this.title}}
                <form class="upgrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default submit" id="upgrade">Promote!
                    </button> </form>
            </p>
            {{/each}}

        </ul>
    </div>

    <div class="col-sm-6" style="text-align: center;">
        <h3 class="master-list">Manager List</h3>
         <br>
        <ul>
            {{#each usermanager}}
            <p id="manager">{{this.email}} - {{this.title}}
                <form class="degrade" name="{{this.email}}">
                    <button type="submit" class="btn btn-default submit" id="degrade">Demote!
                    </button> </form>
            </p>
            {{/each}}

        </ul>
    </div>

    
</div>

<div class="row" style="text-align: center;">
    <a href="/logout"><h2>- Logout -</h2></a>
</div>
<script src="/js/master.js"></script>